<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.GoodsTypeDao">
    <select id="selectGoodsType" resultType="com.po.GoodsType">
      select * from goodstype
    </select>
    <select id="selectGoodsTypeById" parameterType="Integer" resultType="com.po.GoodsType">
      select * from goodstype where id=#{id}
    </select>
</mapper>